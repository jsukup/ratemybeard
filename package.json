{
  "name": "ratemyfeet-workspace",
  "version": "2.0.0",
  "description": "RateMyFeet monorepo workspace containing web and mobile applications",
  "private": true,
  "workspaces": [
    "web",
    "mobile",
    "shared"
  ],
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "npm run dev --workspace=web",
    "dev:web": "npm run dev --workspace=web",
    "dev:mobile": "npm run start --workspace=mobile",
    "build": "npm run build --workspaces --if-present",
    "build:web": "npm run build --workspace=web",
    "build:mobile": "npm run build --workspace=mobile",
    "build:shared": "npm run build --workspace=shared",
    "lint": "npm run lint --workspaces --if-present",
    "lint:fix": "npm run lint:fix --workspaces --if-present",
    "type-check": "npm run type-check --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "clean:deps": "rm -rf node_modules web/node_modules mobile/node_modules shared/node_modules",
    "fresh-install": "npm run clean:deps && npm install",
    "workspace:web": "npm --workspace=web",
    "workspace:mobile": "npm --workspace=mobile",
    "workspace:shared": "npm --workspace=shared",
    "postinstall": "npm run build:shared"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "zod": "^3.23.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsukup/ratemyfeet.git"
  },
  "keywords": [
    "ratemyfeet",
    "rating",
    "mobile",
    "web",
    "supabase",
    "nextjs",
    "react-native",
    "expo"
  ],
  "author": "RateMyFeet Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsukup/ratemyfeet/issues"
  },
  "homepage": "https://ratemyfeet.net"
}